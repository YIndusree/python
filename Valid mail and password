email="vidyashreea@gmail.com"
password=1234
otp=8951
Email=str(input('Enter E-mail:'))
Password=int(input('Enter Password:'))
if(email==Email):
    if(password==Password):
        print('Login Success')
        OTP=int(input('Enter OTP:'))
        if(otp==OTP):
             print('Transaction Successful')
        else:
            print('Transaction failed due to invalid OTP')
    else:
         print('Login faild due to incorrect password')
else:
    print('login faild due to incorrect email')
